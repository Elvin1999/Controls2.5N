<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:sys="clr-namespace:System;assembly=mscorlib">
   
    <SolidColorBrush x:Key="maincolor">Red</SolidColorBrush>
    <LinearGradientBrush x:Key="specialColor">
        <GradientStop Color="DarkGoldenrod" Offset="0"></GradientStop>
        <GradientStop Color="Black" Offset="1"></GradientStop>
    </LinearGradientBrush>
    <sys:Double x:Key="mysize">60</sys:Double>

    <FontFamily x:Key="myCommonFont"  >Comic Sans MS</FontFamily>
    
</ResourceDictionary>